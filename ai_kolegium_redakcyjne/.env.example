# OpenAI API Key
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Serper API for web search
# SERPER_API_KEY=your_serper_api_key_here

# CrewAI Configuration
CREWAI_STORAGE_LOG_ENABLED=false
CREWAI_LOG_LEVEL=WARNING

# Reduce memory operation logs
CREWAI_MEMORY_LOG_ENABLED=false

# Control agent verbosity (0=silent, 1=minimal, 2=normal, 3=verbose)
CREWAI_AGENT_VERBOSITY=1